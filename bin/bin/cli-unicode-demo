#!/bin/bash

set -euo pipefail

MAZE="\
╔════╦══════╦══╗
╠═══╗║╠═══╦╗║╚╗║
║╔═╗╬╠═╦═╣║║╚╚║║
║╚╗╠╚╦║║╔═╣╚═╦╝║
║║║╝╔═╩╝║╠╝╣╔╝╔╣
║║╚═╣╔══╣╔═╝║╩╣║
║╚═╗╚╝╣║╝║╝═╩╔╩║
╚══╩══╩══╩═════╝
"
echo "$MAZE" | lolcat

INNER="\
╚══╦╣  ╠═╦╗║ ╠
╔═╗╬╠═╦═╣║║╚╚║
╚╗╠╚╦║║╔═╣╚═╦╝
╣║╝╔═╩╝║╠╝╣╔╝╔
║╚═╝╔══╣ ═╝║╩╣"

