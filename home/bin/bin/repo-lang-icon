#!/bin/bash

if   test -f "${1:-$PWD}/requirements.txt"; then echo "${2:-#[fg=colour226}󰌠 "
elif test -f "${1:-$PWD}/Gemfile";          then echo "${2:-#[fg=colour196}${2:-}󰴭 "
elif test -f "${1:-$PWD}/tsconfig.json";    then echo "${2:-#[fg=colour81}󰛦 "
elif test -f "${1:-$PWD}/package.json";     then echo "${2:-#[fg=colour220} "
elif test -f "${1:-$PWD}/go.mod";           then echo "${2:-#[fg=colour46}󰟓 "
else
  echo "${2:-#[fg=colour226]} "
fi
